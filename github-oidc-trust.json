{
    "Version":  "2012-10-17",
    "Statement":  [
                      {
                          "Effect":  "Allow",
                          "Principal":  {
                                            "Federated":  "arn:aws:iam::637423256673:oidc-provider/token.actions.githubusercontent.com"
                                        },
                          "Condition":  {
                                            "StringLike":  {
                                                               "token.actions.githubusercontent.com:sub":  "repo:angelcreator113/lala-infra:*"
                                                           },
                                            "StringEquals":  {
                                                                 "token.actions.githubusercontent.com:aud":  "sts.amazonaws.com"
                                                             }
                                        },
                          "Action":  "sts:AssumeRoleWithWebIdentity"
                      }
                  ]
}
