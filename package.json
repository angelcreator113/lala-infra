{
  "name": "aws-lala-starter",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy --all",
    "diff": "cdk diff",
    "cf:invalidate": "powershell -ExecutionPolicy Bypass -File scripts/cf-invalidate.ps1 -AwsProfile lala-keys -StackName LalaWebStack -Region us-east-1",
    "cf:pages": "npm run cf:invalidate -- -Paths \"/index.html,/robots.txt,/sitemap.xml\"",
    "cf:all": "npm run cf:invalidate -- -Paths \"/*\"",
    "publish:fast": "powershell -ExecutionPolicy Bypass -File scripts/s3-publish.ps1 -AwsProfile lala-keys -StackName LalaWebStack -Region us-east-1",
    "publish:fast:wait": "npm run publish:fast -- -Wait",
    "publish:fast:all": "npm run publish:fast -- -Paths \"/*\" -Wait"
  },
  "devDependencies": {
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.21",
    "cdk-nag": "^2.28.0",
    "postcss": "^8.5.6",
    "source-map-support": "^0.5.21",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "5.4",
    "vite": "^7.1.6"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.156.0",
    "constructs": "^10.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1"
  }
}
